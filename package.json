{
  "name": "skok-photo-manager",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently -n \"ELECTRON,APP\" -c \"blue,cyan\" \"npm run start:electron\" \"npm run start:web\"",
    "start:electron": "onchange -i -k \"src/**/*.ts\" -- npm run electron",
    "start:web": "parcel ./app/index.html --public-url . --no-hmr --target electron",
    "electron": "tsc && electron ./build/app.js",
    "postinstall": "electron-builder install-app-deps",
    "test": "ts-node ./tests/indexPhotos.test.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "10.12.18",
    "babel-core": "6.26.3",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "electron": "^4.0.0",
    "electron-builder": "20.38.4",
    "sass": "1.15.2",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "husky": "1.3.1",
    "knex": "0.16.3",
    "lint-staged": "8.1.0",
    "nanoid": "2.0.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "sqlite3": "4.0.4",
    "walker": "1.0.7"
  },
  "husky": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,json,md,ts": [
      "prettier --write",
      "git add"
    ]
  }
}
